  ingredients          Json     @default("[]")
  total_cost           Decimal  @default(0) @db.Decimal
  batch_size           Decimal? @db.Decimal
  number_of_units      Int?
  cost_per_unit        Decimal? @db.Decimal
  original_batch_size  Decimal? @db.Decimal
