generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
  schemas  = ["auth", "public"]
}

model RawMaterial {
  id            String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name          String
  total_cost    Decimal  @db.Decimal
  total_weight  Decimal  @db.Decimal
  weight_unit   String
  cost_per_gram Decimal  @db.Decimal
  supplier_name String?
  supplier_contact String?
  last_purchase_date DateTime?
  purchase_notes String?
  usage_notes String?
  typical_monthly_usage Decimal? @db.Decimal
  user_id       String   @db.Uuid
  created_at    DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  updated_at    DateTime @default(dbgenerated("timezone('utc'::text, now())")) @updatedAt @db.Timestamptz(6)
  users         users    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([user_id], map: "idx_raw_materials_user_id")
  @@map("raw_materials")
  @@schema("public")
}

model PackagingItem {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  name        String
  cost        Decimal  @db.Decimal
  description String?
  supplier    String?
  category    String?
  user_id     String   @db.Uuid
  created_at  DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  updated_at  DateTime @default(dbgenerated("timezone('utc'::text, now())")) @updatedAt @db.Timestamptz(6)
  users       users    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([user_id], map: "idx_packaging_items_user_id")
  @@map("packaging_items")
  @@schema("public")
}

